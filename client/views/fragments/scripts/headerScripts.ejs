<script src="/node/vanilla-lazyload/dist/lazyload.min.js" type="text/javascript"></script>

<script>
	$(function () {
		action_titleAppendCounter(<%= it.data.notifs.length %>);
		action_loadLastActivity();
			
		var socket = io();
		socket.on('notif', function(data){
			action_newNotification(data);
		});
		socket.on('activity', function(data){
			switch(data.kind){
				case "comment":
					action_appendActivity(data.data);
				break;
				case "box":
					//no implementado
				break;
			}
		});
		
	});
</script>

